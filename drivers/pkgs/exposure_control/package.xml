<?xml version="1.0"?>
<package format="2">
  <name>exposure_control</name>
  <version>0.0.1</version>
  <description>control camera exposure</description>
  <maintainer email="bernd.pfrommer@gmail.com">ke</maintainer>

  <license>BSD</license>
  <buildtool_depend>catkin</buildtool_depend>

  <!-- dependencies -->
  <depend>roscpp</depend>
  <depend>sensor_msgs</depend>
  <depend>nodelet</depend>
  <depend>dynamic_reconfigure</depend>

  <!-- for CamTest node -->
  <depend>flea3</depend>
  <depend>camera_base</depend>

  <!-- exports -->
  <export>
    <nodelet plugin="${prefix}/nodelet_plugins.xml"/>
  </export>

</package>
